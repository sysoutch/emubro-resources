      "setupFileMatchLinux": "",
      "setupFileMatchMac": "",
      "executableFileMatchWin": "^(retroarch)\\.exe$",
      "executableFileMatchLinux": "^(retroarch(.*))\\.(AppImage|appimage)$",
      "executableFileMatchMac": "^(retroarch(.*))\\.app$",
      "autoSearchEnabled": true,
      "biosRequired": true,
      "runCommandsBefore": [],
      "downloadUrl": {
        "win": [
          "https://github.com/libretro/RetroArch/releases/latest"
        ],
        "mac": [
          "https://github.com/libretro/RetroArch/releases/latest"
        ],
        "linux": [
          "https://github.com/libretro/RetroArch/releases/latest"
        ]
      },
      "installers": {
        "linux": {
          "flatpak": {
            "id": "org.libretro.RetroArch",
            "remote": "flathub",
            "install": "flatpak install -y flathub org.libretro.RetroArch",
            "run": "flatpak run org.libretro.RetroArch"
          },
          "apt": {
            "packages": [
              "retroarch"
            ],
            "install": "sudo apt install -y retroarch",
            "run": "retroarch"
          }
        }
      }
    },
